#%RAML 1.0 DataType
type: object
properties:
  id: integer
  place_name: string
  place_hierarchies:
    type: array
    items:
      type: object
      properties:
        id: integer
        hierarchy_name: string
        beacons:
          type: array
          items:
            type: object
            properties:
              id: number
              beacon_name: string
              beacon_type: string
              beacon_radius: string
