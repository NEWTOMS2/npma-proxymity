		<http:listener-connection protocol="HTTPS" host="${http.experience.host}" port="${http.experience.port}" >
			<tls:context >
				<tls:key-store type="jks" path="keystore.jks" keyPassword="password" password="password" />
			</tls:context>
			<tls:context >
				<tls:key-store type="jks" path="keystore.jks" keyPassword="password" password="password" />
			</tls:context>
		</http:listener-connection>
	</http:listener-config>
	<api-gateway:autodiscovery apiId="15539052" doc:name="API Autodiscovery" doc:id="1459b5e2-11e7-46f6-b08c-144d6ae45233" flowRef="api-main" />
